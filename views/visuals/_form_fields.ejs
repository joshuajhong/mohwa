<div class="form-group">
    <label for="name">Name</label>
    <input required value="<%= visual.name %>"  type="text" name="name" id="name" class="form-control">
</div>
<div class="form-group">
    <label for="image">Image</label>
    <input type="file" name="image" class="form-control-file">
</div>
<div class="form-group">
    <label for="description">Description</label>
    <input required value="<%= visual.description %>"  type="text" name="description" id="description" class="form-control">
</div>
<div class="form-group">
    <label for="keywords">Keywords</label>
    <input required value="<%= visual.keywords %>"  type="text" name="keywords" id="keywords" class="form-control">
</div>


<a href="/adminpanel" class="btn btn-danger"><i class="fas fa-window-close"></i></a>
<button type="submit" class="btn btn-success"><i class="fas fa-save"></i></button>
