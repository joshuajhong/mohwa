<head>
  <title>Mohwa | bookings</title>
</head>

<body>
  <nav class="navbar navbar-expand-md fixed-tm-navbar">
    <div class="container-fluid">
        <a href="/"><button accesskey="h" class="homebutton">
            <img src="images/logo.png" class="img-fluid logo-image" alt="MOHWA logo">
        </button></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fas fa-bars navbar-toggler-icon"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto">
            <li class="nav-item">
                <a class="nav-link tm-nav-link" href="/">HOME</a>
            </li>
            <li class="nav-item">
                <a class="nav-link tm-nav-link" href="../../blog">BLOG</a>
            </li>
            <li class="nav-item">
                <a class="nav-link tm-nav-link" href="/contact">CONTACT</a>
            </li>
            <li class="nav-item">
                <a class="nav-link tm-nav-link currenttab" href="/bookings">BOOKINGS</a>
            </li>
        </ul>
        </div>
    </div>
  </nav> 
  <section id="bookings" class="tm-section-pad-top tm-parallax-2">
    <div class="container tm-container-contact">
      <div class="row">
          <div class="text-center col-12">
              <h2 class="tm-section-title mb-4">BOOKINGS</h2>
              <p class="mb-5">Book MOHWA at house concerts, intimate venues, bars, weddings, corporate and special events.</p>
          </div>

      <div class="col-sm-12 col-md-12 animate__animated animate__bounceInLeft">
        <form action="/bookings" method="post" id="g-recaptcha-response">
          <div class="bookingsection">
            <h3>Contact details</h3>
            <input id="name" name="name" type="text" placeholder="Name" class="tm-input" required />
            <input id="email" name="email" type="email" placeholder="Email" class="tm-input" required />
            <input id="phone" name="phone" type="tel" placeholder="Phone Number" class="tm-input" required />
          </div>
          <div class="bookingsection">
            <h3>Select a date and time</h3>
            <input id="date" name="date" type="date" placeholder="Select a date" class="tm-input2" required />
            <input id="time" name="time" type="time" placeholder="Select a time" class="tm-input2" required />
          </div>
          <div class="bookingsection">
            <h3>Venue details</h3>
            <input id="venue" name="venue" type="text" placeholder="Venue Name" class="tm-input" required />
            <input id="address" name="address" type="text" placeholder="Address Line 1" class="tm-input" required />
            <input id="venueAddress2" name="venueAddress2" type="text" placeholder="Address Line 2" class="tm-input" />
            <input id="suburb" name="suburb" type="text" placeholder="Suburb" class="tm-input2" />
            <input id="postcode" name="postcode" type="tel" pattern="[0-9]{4}" placeholder="Postcode" class="tm-input2" required />
            <select id="state" name="state" class="tm-input2" required>
                <option disabled selected value> -- State -- </option>
                <option value="New South Wales">New South Wales</option>
                <option value="Victoria">Victoria</option>
                <option value="Queensland">Queensland</option>
                <option value="South Australia">South Australia</option>
                <option value="Western Australia">Western Australia</option>
                <option value="Tasmania">Tasmania</option>
                <option value="Australian Capital Territory">Australian Capital Territory</option>
                <option value="Northern Territory">Northern Territory</option>
            </select>
          </div>
          <h3>Message</h3>
            <textarea id="message" name="message" rows="8" placeholder="Write down any details here" class="tm-input"></textarea>
            <title>reCAPTCHA demo: Explicit render after an onload callback</title>
            <script type="text/javascript">
              var onloadCallback = function() {
                grecaptcha.render('html_element', {
                  'sitekey' : '6LesDbYZAAAAAH3zVJTnxv3wN3zUj7dYkLYg9Syc'
                });
              };
            </script>
          <div id="html_element"></div>
          <br>
          <input type="submit" value="Submit" class="contact-button">
        </form>
        <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit"
            async defer>
        </script>
      </div>
    </div>
  </div>
</section>

<script src="vendor/jquery/jquery-3.5.1.min.js"></script>
<script src="vendor/bootstrap/js/bootstrap.min.js"></script>
<script>
  $("form").submit(function(event) {
    var recaptcha = $("#g-recaptcha-response").val();
    if (recaptcha === "") {
       event.preventDefault();
       alert("Please check the recaptcha");
        }
    });
 </script>
</body>
